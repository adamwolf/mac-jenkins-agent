<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>EnvironmentVariables</key>
    <dict>
      <key>PATH</key>
      <string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:</string>
    </dict>
    <key>WorkingDirectory</key>
    <string>/Users/jenkinsbuilder/mac-jenkins-agent</string>
    <key>Label</key>
    <string>org.adamwolf.mac-jenkins-agent</string>
    <key>Program</key>
    <string>/Users/jenkinsbuilder/mac-jenkins-agent/mac-jenkins-agent.sh</string>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>ThrottleInterval</key><!-- Wait this long to restart the job, if it exits -->
    <integer>30</integer>
    <key>StandardOutPath</key>
    <string>/tmp/org.adamwolf.mac-jenkins-agent.stdout</string>
    <key>StandardErrorPath</key>
    <string>/tmp/org.adamwolf.mac-jenkins-agent.stderr</string>
    <key>UserName</key>
    <string>jenkinsbuilder</string>
    <key>InitGroups</key>
    <true/>
  </dict>
</plist>
